unsigned long nextLEDChange = 0;
unsigned long nextDetectionProbe = 0;
unsigned long nextSampleEnd = 0;

unsigned long presenceConfidenceCounter = 0;
unsigned long laughCounter = 0;
unsigned long lastLaugh = 0;

boolean presence = false;

void setup(){
  noTone(PIEZO_PIN);
  pinMode(TRIGGER_PIN, OUTPUT); 
  pinMode(ECHO_PIN, INPUT); 
  pinMode(RED_LEDS_PIN, OUTPUT); 
  pinMode(RED_CANDLE_LED_PIN, OUTPUT); 
  pinMode(YELLOW_CANDLE_LED_PIN, OUTPUT); 
  Serial.begin(9600);
}

void loop()
{
  if(laughCounter > 0 && millis() - lastLaugh >= MIN_LAUGH_PAUSE)
  {
      laughCounter = 0;
  }  
 
  if(laughCounter < MAX_LAUGHS_IN_A_ROW && detectPresence())
  {
      ledDanger();
      laughCounter++;
      laugh();      
  } else {
      ledCandle();    
  }  
}  

boolean detectPresence()
{
  
  unsigned long currentMillis = millis();
  if(currentMillis < nextDetectionProbe)
  {
    return presence;
  }

  if(getDistanceInCm() > DISTANCE_CM_THRESHOLD)
  {
      presenceConfidenceCounter = 0;
      presence = false;
  } else {
      presenceConfidenceCounter++;
      if(presenceConfidenceCounter<PRESENCE_CONFIDENCE_THRESHOLD){
        presence = false;
      } else { 
        presence = true;
        presenceConfidenceCounter=0;
      }
  }    
  
  nextDetectionProbe = currentMillis + PRESENCE_DETECTION_INTERVAL;
  return presence;
}  

long getDistanceInCm()
{
   digitalWrite(TRIGGER_PIN, LOW); 
   delayMicroseconds(2); 
   digitalWrite(TRIGGER_PIN, HIGH);
   delayMicroseconds(10); 
   digitalWrite(TRIGGER_PIN, LOW);

   long duration = pulseIn(ECHO_PIN, HIGH);
   long distance = duration/58.2;

   Serial.println(distance); 
   return distance;

}  

void laugh()
{
      Serial.print("Play[");
      Serial.print(laughCounter);
      Serial.println("] ...");
      startPlayback(sample, sizeof(sample));
      delay(SAMPLE_LEN);
      Serial.println("Stop ...");      
      stopPlayback();
      
      lastLaugh = millis();;
}  

void releaseSound()
{
  stopPlayback();
}  

void ledCandle()
{
    digitalWrite(RED_LEDS_PIN, LOW);
   
    unsigned long currentMillis = millis();
    if(currentMillis > nextLEDChange)
    {
	analogWrite(RED_CANDLE_LED_PIN, random(80,255));
	analogWrite(YELLOW_CANDLE_LED_PIN, random(80,255));
	nextLEDChange = currentMillis + random(MAX_CANDLE_UPDATE_INTERVAL);
    }
}  

void ledDanger()
{
      digitalWrite(RED_LEDS_PIN, HIGH);
      digitalWrite(RED_CANDLE_LED_PIN, HIGH);
      digitalWrite(YELLOW_CANDLE_LED_PIN, LOW);
}  


